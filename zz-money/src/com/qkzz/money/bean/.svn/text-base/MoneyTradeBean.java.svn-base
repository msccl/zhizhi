package com.qkzz.money.bean;

public class MoneyTradeBean {
	private int id;
	private int role;//角色对应
	private int transtype;//0： 减少金额，1： 增加金额
	private String transcontent;//交易内容模板
	private String description;//描述
	private int status;//记录状态 0：正常使用 1：等待审核
	private String updatetime;//每次更改的时间
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getRole() {
		return role;
	}
	public void setRole(int role) {
		this.role = role;
	}
	public int getTranstype() {
		return transtype;
	}
	public void setTranstype(int transtype) {
		this.transtype = transtype;
	}
	public String getTranscontent() {
		return transcontent;
	}
	public void setTranscontent(String transcontent) {
		this.transcontent = transcontent;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public String getUpdatetime() {
		return updatetime;
	}
	public void setUpdatetime(String updatetime) {
		this.updatetime = updatetime;
	}
}
